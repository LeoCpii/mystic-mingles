{
  "name": "@mingles/lab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:prod": "env-cmd -f .env.prod vite --port 4000",
    "start:test": "env-cmd -f .env.test vite --port 4000",
    "start:local": "env-cmd -f .env.local vite --port 4000",
    "start:stg": "env-cmd -f .env.staging vite --port 4000",
    "build:prod": "tsc && env-cmd -f .env.prod vite build",
    "build:stg": "tsc && env-cmd -f .env.staging vite build",
    "build:local": "tsc && env-cmd -f .env.local vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@firebase/auth": "^1.3.0",
    "@mui/icons-material": "^5.14.13",
    "@mui/lab": "^5.0.0-alpha.129",
    "@mui/material": "^5.14.13",
    "@sentry/react": "^7.74.0",
    "env-cmd": "^10.1.0",
    "firebase": "^10.5.0",
    "notistack": "^3.0.1",
    "react": "18",
    "react-dom": "18",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "resolutions": {
    "@types/react": "^17.0.38"
  }
}
